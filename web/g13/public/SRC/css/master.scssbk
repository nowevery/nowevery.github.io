.quality-slick {
    width: 85%;
    margin: auto;

    img {
        width: 100%;
        padding: 7%;
    }

    .slick-prev {
        left: -8%;

        &:before {
            content: "\f053";
            font-weight: 900;
            font-family: "Font Awesome 5 Free";
            -webkit-font-smoothing: antialiased;
            display: inline-block;
            font-style: normal;
            font-variant: normal;
            text-rendering: auto;
            line-height: 1;
            color: $blue;
            top: 13px;
            transform: rotate(0deg);
            font-size: 16px;
        }

        @media screen and (max-width: 480px) {
            left: -12%;
        }

    }

    .slick-arrow {
        border: 1px solid $blue;
        color: $blue;

        &:hover {

            &:before {
                color: #fff;
            }
        }
    }

    .slick-next {
        right: -8%;

        &:before {
            content: "\f054";
            font-weight: 900;
            font-family: "Font Awesome 5 Free";
            -webkit-font-smoothing: antialiased;
            display: inline-block;
            font-style: normal;
            font-variant: normal;
            text-rendering: auto;
            line-height: 1;
            color: $blue;
            top: 13px;
            font-size: 16px
        }

        @media screen and (max-width: 480px) {
            right: -12%;
        }
    }

    .slick-arrow {
        font-weight: 900;
        font-family: "Font Awesome 5 Free";
        -webkit-font-smoothing: antialiased;
        display: inline-block;
        font-style: normal;
        font-variant: normal;
        text-rendering: auto;
        line-height: 1;
    }

    .slick-prev,
    .slick-next {
        z-index: 1;

        &:hover,
        &:focus {
            outline: none;
            background: #006dc8;
            transition: .3s;
        }

    }
}

