    }
}

.form-box {
    .text {
        font-size: 14px;
        color: #666;
        line-height: 24px;
        margin-bottom: 20px;
        display: flex;
    }
}

.line-flex {
    display: flex;
    margin-bottom: 5px;
    color: #000;
}

.tip-icon {
    color: $blue;
    font-size: 15px;
    margin-right: 5px;
    display: inline-block;
}

input {
    border: 1px solid #b7b7b7;
    line-height: 40px;
}

textarea {
    border: 1px solid #b7b7b7;
}

.btn {
    padding: 0px 5%;
    color: #fff;
    background-color: $blue;
    border: 0;
    line-height: 45px;
    max-width: 200px;
    width: 45%;
    text-transform: uppercase;
    font-size: 15px;
    font-weight: 700;
    letter-spacing: 1px;

    &:hover {
        background-color: #269dff;
        @include transition(.5s)
    }
}

.one-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.two-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    white-space: normal;
}

.sp-font {
    @include Oswald();

}

body {
    font-family: arial, sans-serif;
}

header {
    width: 100%;
    background-color: rgba(255, 255, 255, 1);
    z-index: 8;
    padding: 35px 0 20px;
    position: sticky;
    top: 0;
    z-index: 5;

    >.inner {
        display: inline-flex;
        justify-content: space-between;
        max-width: 100%;
        padding: 0 2.5%;
    }
}


.logo {
    max-width: 288px;
    display: inline-block;
    margin-right: 35px;
}

.logo a {
    display: block;
}

.logo img {
    width: 100%;
    max-height: 50px;
}

.nav-bar {
    cursor: pointer;
    position: relative;
    top: 12px;

    @media screen and (max-width: 480px) {
        margin-right: 5px;
    }

    span {
        width: 20px;
        height: 3px;
        margin-bottom: 5px;
        background: #666;
        display: block;
    }

    &.close {
        span {
            opacity: 0;
            position: relative;
            @include transition(.3s);
            background: $blue;

            &:first-of-type {
                opacity: 1;
                transform: rotate(45deg);
                top: 8px;


            }

            &:last-of-type {
                opacity: 1;
                transform: rotate(-45deg);
                top: -8px;
            }
        }

    }
}



.top-menu {
    margin-top: 25px;
    width: calc(100% - 290px);

    text-transform: uppercase;
    justify-content: flex-end;


    @media screen and (max-width: 1400px) {
        margin-top: 20px;
        font-size: 14px;
    }



    @media screen and (min-width: 1200px) {
        display: flex !important;

        li {
            font-weight: 700;
            margin-left: 2.5%;
            display: inline-block;
            position: relative;

            >a {
                line-height: 40px;
                display: block;
            }

            &:hover {
                >a {
                    color: $blue;
                    @include transition(.5s);
                }

                .menu-drop {
                    height: auto;
                    opacity: 1;
                    border: 1px solid #eee;
                }
            }
        }
    }

    .menu-drop {
        text-transform: initial;

        @media screen and (min-width: 1201px) {
            height: 0;
            overflow: hidden;
            @include transition(.2s);
            position: absolute;
            left: 0;
            opacity: 0;
            z-index: 1;
            border: 1px solid transparent;

            a {
                font-size: 12px;
                width: 180px;
                background: #fff;
                font-weight: 100;
                padding: 12px 15px;
                border-bottom: 1px solid #eee;
                color: #666;
                line-height: 18px;
                display: block;

                &:last-of-type {
                    border-bottom: 0;
                }

                &:hover {
                    background: #eee;
                    color: $blue;
                }
            }
        }
    }

    @media screen and (max-width: 1200px) {
        position: fixed;
        flex-wrap: wrap;
        background: #fff;
        width: 100%;
        left: 0;
        margin: 0;
        top: 65px;
        display: none;
        border: 1px solid #eee;
        overflow-y: scroll;
        max-height: calc(100% - 65px);

        .open {
            display: none;
        }

        li {
            display: block;
            width: 100%;
            margin: 0;
            border-bottom: 1px solid #eee;


            .drop {
                padding: 15px 20px;
                display: block;
                border-bottom: 1px solid #eee;
                line-height: inherit;
                position: relative;
            }

            .fas {
                position: absolute;
                right: 15px;
                top: calc(50% - 7px);
                color: $blue;
            }
        }

        .menu-drop {
            display: none;

            a {
                padding: 15px 35px;
                display: block;
                font-weight: 400;
                border-bottom: 1px solid #eee;
                background: #f9f9f9;
                color: #8a8a8a;

                &:last-of-type {
                    border-bottom: 0;
                }
            }
        }

    }
}

@media screen and (max-width: 1400px) {
    .logo {
        max-width: 200px;
    }


}

@media screen and (max-width: 1200px) {

    .logo {
        max-width: 175px;
    }

    header {
        padding: 17px 0 17px;
    }

    .m-bg {
        position: fixed;
        width: 100%;
        height: 100%;
        background: rgb(109, 132, 150, .5);
        left: 0;
        top: 0;
        display: none;
        z-index: 3;
    }
}

.shortcut {
    background: $blue;
    font-size: 15px;
    position: sticky;
    top: 120px;
    z-index: 3;

    @media screen and (max-width: 1400px) {
        top: 115px;
    }

    @media screen and (max-width: 1200px) {
        top: 65px;
    }
}

.shortcut>.inner {
    display: flex;
}

@media screen and (max-width: 1400px) {
    .shortcut>.inner {
        padding: 0;
    }
}

.shortcut img {
    width: 20px;
    margin-right: 5px;
    filter: grayscale(1) invert(1) contrast(6);
}

.shortcut li {
    width: 25%;
    display: inline-block;
    position: relative;
}

.shortcut li:hover,
.shortcut li.active {
    background: #eee;
    transition: 0.5s;
}

.shortcut li:hover a,
.shortcut li.active a {
    color: $blue;
    transition: 0.5s;
}

.shortcut li:hover img,
.shortcut li.active img {
    filter: inherit;
    transition: 0.5s;
}

.shortcut li:hover+li:after,
.shortcut li.active+li:after {
    opacity: 0;
}

.shortcut a {
    color: #fff;
    text-align: center;
    padding: 17px 0;
    position: relative;
    display: block;
}

.shortcut li:not(:first-of-type):after {
    content: '';
    width: 1px;
    height: 25px;
    background: rgba(255, 255, 255, 0.5);
    position: absolute;
    left: 0;
    top: 15px;
    display: block;
}

@media screen and (max-width: 650px) {
    .shortcut>.inner {
        flex-wrap: wrap;
    }

    .shortcut li {
        width: 50%;
    }

    .shortcut li:nth-of-type(odd) {
        border-right: 1px solid rgba(255, 255, 255, 0.5);
    }

    .shortcut li:nth-of-type(-n+2) {
        border-bottom: 1px solid rgba(255, 255, 255, 0.5);
    }

    .shortcut li:not(:first-of-type):after {
        display: none;
    }

    .shortcut a {
        padding: 13px 0;
        font-size: 13px;
    }

    .shortcut img {
        width: 15px;
    }
}

.bread {
    font-size: 12px;
    color: #999;
    border: 1px solid #e2e2e2;
    border-left: 0;
    border-right: 0;
    padding: 20px 0;
    background: #fff;
}

.bread a {
    color: #999;
}

.bread-space {
    margin: 0 10px;
}

footer .inner {
    padding: 75px 0 85px;
    display: flex;
    max-width: 1250px;
}

footer .inner>div {
    width: 33%;
}

.footer-btn {
    width: 100%;
    padding-right: calc(33% - 260px);
    font-size: 16px;
    letter-spacing: 1px;
}

.footer-btn a {
    display: block;
    background: $blue;
    color: #fff;
    margin-bottom: 38px;
}

.footer-btn a:hover {
    background: #eee;
    transition: 0.5s;
    color: $blue;
    transition: 0.5s;
}

.footer-btn a:hover img {
    filter: inherit;
    transition: 0.5s;
}

.footer-btn img {
    width: 20px;
    height: 50px;
    filter: grayscale(1) invert(1) contrast(6);
    margin: 0 15px;
}

.footer-btn span {
    line-height: 50px;
    border-left: 1px solid rgba(225, 225, 225, 0.4);
    display: inline-block;
    padding: 0 20px;
}

.footer-resources {
    width: 100%;
    padding-right: calc(33% - 370px);

    a {
        font-size: 18px;
        padding: 22px 0 22px 30px;
        width: 100%;
        display: inline-block;
        color: #666;
        border-bottom: 1px solid #ccc;
        position: relative;

        &:last-of-type {
            border-bottom: 0px;
        }

        &:before {
            content: '';
            width: 10px;
            height: 2px;
            background: $blue;
            display: block;
            position: absolute;
            left: 0;
            top: 29px;
        }

        &:hover {
            color: $blue;
            transition: 0.3s;
        }
    }
}

.footer-contact {
    padding-left: calc(33% - 335px);
}

.footer-contact img {
    width: 20px;
    margin-right: 20px;
}

.footer-contact .title {
    margin: 28px 0 15px;
    font-size: 18px;
}

.footer-contact .title+div {
    display: flex;
    color: #999;
    font-size: 15px;

    a {
        color: #999;
        font-size: 15px;
        margin: 0;
    }
}

.footer-contact a {
    display: flex;
    font-size: 24px;
    margin: 20px 0;
    color: $blue;

    &:hover {
        opacity: .7;
        @include transition(.5s);
    }
}

.footer-title {
    font-size: 26px;
    color: $blue;
    margin-bottom: 8px;
}

.copyright {
    font-size: 13px;
    color: #b1b1b1;
    padding: 28px 0;
    background: #666;
    text-align: center;

    @media screen and (max-width: 410px) {
        font-size: 0;
    }
}

.copyright span {
    margin-right: 15px;
    font-size: 13px;
}

.copyright a {
    color: #b1b1b1;
    margin-left: 15px;
    font-size: 13px;
}

.copyright a:hover {
    color: #fff;
}

@media screen and (max-width: 1200px) {
    footer .inner {
        padding: 5% 20px;
        flex-wrap: wrap;
        max-width: 850px;
    }

    footer .inner>div {
        width: 50%;
    }

    .footer-btn {
        display: none;
    }

    .footer-resources {
        min-width: 370px;
        padding-right: calc(50% - 370px);
    }

    .footer-contact {
        min-width: 370px;
        padding-left: calc(50% - 330px);
    }
}

@media screen and (max-width: 780px) {
    footer .inner {
        padding: 0 20px;
    }

    footer .inner>div {
        width: 100%;
    }

    .footer-resources,
    .footer-contact {
        padding: 0;
        margin: 30px 0;
    }

    .copyright {
        padding: 20px 0;
        line-height: 24px;
    }
}

@media screen and (max-width: 480px) {
    footer .inner>div {
        width: 100%;
    }

    .footer-title {
        font-size: 24px;
    }

    .footer-resources,
    .footer-contact {
        min-width: auto;
        max-width: 370px;
        width: 100%;
    }

    .footer-contact a {
        font-size: 15px;
    }

    .footer-contact img {
        width: 15px;
        align-self: center;
    }

    .copyright a {
        display: inline-block;
    }
}


/*contact*/
.contact-info {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;

    iframe {
        width: 50%;

        @media screen and (max-width: 768px) {
            margin-bottom: 20px;
        }
    }

    .info-right {
        width: 45%;
        display: flex;
        flex-direction: column;

        i {
            color: $blue;
            margin-right: 10px;
        }

        >div {
            margin-bottom: 25px;
            color: #666;
            display: flex;
        }
    }

    a {
        color: #666;

        &:hover {
            color: $blue;
            @include transition(.5s)
        }
    }

    img {
        width: 20px;
        margin-right: 10px;
    }

    .logo-pic {
        width: 100%;
        max-width: 220px;
        margin-bottom: 15px;
        margin-top: 25px;
    }

    @media screen and (max-width: 768px) {
        flex-direction: column;

        iframe {
            width: 100%;
        }

        .info-right {
            width: 100%;
        }
    }
}

.form-box {
    padding: 45px 10%;
    margin: 45px 0 45px;
    border: 1px solid $blue;

    .sub {
        font-size: 20px;
        color: #666;

        span {
            color: $blue;
        }
    }

    .row {
        display: flex;
        flex-direction: column;
        margin: 30px 0;
        justify-content: center;
        // color: #666;
        font-size: 15px;

    }

    input {
        margin-top: 5px;
    }

    span {
        display: flex;
        align-items: center;
    }

    .code {
        position: relative;
        @extend span;

        @media screen and (max-width: 480px) {
            input {
                width: 75%;
            }
        }

        .fas {
            font-size: 18px;
            display: inline-block;
            width: 40px;
            height: 40px;
            line-height: 40px;
            cursor: pointer;

            &:hover {
                opacity: .7;
                @include transition(.5s);
            }
        }

        label.error {
            position: absolute;
            bottom: -18px;
        }

    }

    input.error,
    textarea.error {
        border-color: #fff;
        background: #fee;
    }

    label.error {
        font-size: 12px;
        margin-top: 5px;
        color: #c00;
    }

    img {
        max-height: 40px;
        width: 170px;
    }

    .title {
        color: $blue;
        font-size: 28px;
        margin-bottom: 20px;

    }

    .sub-title {
        color: #000;
        font-size: 20px;
        margin-top: 20px;

        span {
            color: $blue;
            display: inline-block;
            margin-right: 5px;
        }
    }

    button[type='reset'] {
        background-color: #c5c5c5;
        color: #fff;

        &:hover {
            background-color: #666;
            @include transition(.5s);
        }
    }

    textarea {
        min-height: 100px;
    }

    .btn-box {
        margin-top: 50px;

        @media screen and (max-width: 768px) {
            display: flex;
            justify-content: space-between;

            .btn {
                max-width: 100%;
                width: 49%
            }
        }
    }

    .file-box {
        .name {
            margin-bottom: 5px;
            color: #000;

            &+div {
                display: flex;

                @media screen and (max-width: 480px) {
                    flex-wrap: wrap;
                }
            }
        }

        label {
            background: $blue;
            color: #fff;
            line-height: 45px;
            display: inline-block;
            text-align: center;
            cursor: pointer;
            margin-bottom: 10px;
            width: 50%;
            max-width: 250px;

            @media screen and (max-width: 480px) {
                width: 100%;
                max-width: 100%;
            }

            &:hover {
                background-color: #008bff;
                @include transition(.3s);
            }

            .upload-1,
            .upload-2,
            .upload-3 {
                color: #fff;
                text-align: center;
                display: block;

                &.re {
                    background: #004e90;

                    &:hover {
                        background: #008bff;
                        @include transition(.3s);
                    }
                }
            }

            &+span {
                margin-left: 10px;
                color: #ababab;
                margin-bottom: 10px;

                &.re {
                    color: #003e71;
                    font-size: 15px;
                    background: #ebf6ff;
                    padding: 5px 15px;

                    @media screen and (max-width: 480px) {
                        padding: 10px 15px;
                    }
                }

                @media screen and (max-width: 480px) {
                    display: block;
                    margin-left: 0;
                    margin-bottom: 20px;
                }
            }
        }
    }
}

.check-box {
    position: relative;

    &>label {
        cursor: pointer;
        margin: 5px 0;
        color: #797979;

        input[type="checkbox"],
        input[type="radio"] {
            margin-right: 5px;
        }

        &:hover {
            color: $blue;
            @include transition(.3s);
        }
    }

    .line-flex {
        margin-bottom: 10px;
        margin-top: 10px;
    }

    label.error {
        position: absolute;
        bottom: -10px;
        left: 0;
    }

    textarea {
        min-height: 50px;
        margin-bottom: 10px;
