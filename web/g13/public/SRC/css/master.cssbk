}
@media screen and (max-width: 480px) {
  footer .inner > div {
    width: 100%;
  }

  .footer-title {
    font-size: 24px;
  }

  .footer-resources,
.footer-contact {
    min-width: auto;
    max-width: 370px;
    width: 100%;
  }

  .footer-contact a {
    font-size: 15px;
  }

  .footer-contact img {
    width: 15px;
    align-self: center;
  }

  .copyright a {
