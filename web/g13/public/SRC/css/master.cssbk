.form-box {
  padding: 45px 10%;
  margin: 45px 0 45px;
  border: 1px solid #006dc8;
}
.form-box .sub {
  font-size: 20px;
  color: #666;
}
.form-box .sub span, .form-box .sub .code {
  color: #006dc8;
}
.form-box .row {
  display: flex;
  flex-direction: column;
  margin: 30px 0;
  justify-content: center;
  font-size: 15px;
}
.form-box input {
  margin-top: 5px;
}
.form-box span, .form-box .code {
  display: flex;
  align-items: center;
}
.form-box .code {
  position: relative;
}
@media screen and (max-width: 480px) {
  .form-box .code input {
    width: 75%;
  }
}
.form-box .code .fas {
  font-size: 18px;
  display: inline-block;
  width: 40px;
  height: 40px;
  line-height: 40px;
  cursor: pointer;
}
.form-box .code .fas:hover {
