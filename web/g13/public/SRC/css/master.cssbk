@charset "UTF-8";
@import url("https://fonts.googleapis.com/css2?family=Oswald:wght@400&display=swap");
* {
  box-sizing: border-box;
}

img {
  max-width: 100%;
}

a {
  color: #000;
}

a:hover {
  transition: 0.3s;
}

a[href^=tel] {
  color: inherit;
  text-decoration: none;
}

::placeholder {
  color: #bbb;
}

::-webkit-input-placeholder {
  color: #bbb;
}

:-ms-input-placeholder {
  color: #bbb;
}

::-moz-placeholder {
  color: #bbb;
}

input,
button,
select,
textarea {
  outline: none;
}
input:focus,
button:focus,
select:focus,
textarea:focus {
  border: #006dc8 1px solid;
  transition: 0.3s;
  -moz-transition: 0.3s;
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
}

input,
textarea {
  padding: 0 5px;
}

textarea {
  padding: 5px;
}

input:not([type=button]):focus,
textarea:focus {
  transition: 0.3s;
}

input:-webkit-autofill,
textarea:-webkit-autofill,
select:-webkit-autofill {
  -webkit-box-shadow: 0 0;
}

input[type=text]:focus,
input[type=password]:focus,
textarea:focus {
  -webkit-box-shadow: 0 0;
}

button {
  cursor: pointer;
}

input:not([type=checkbox]):not([type=radio]),
textarea:not([type=checkbox]):not([type=radio]) {
  border-radius: 0;
  -webkit-appearance: none;
}

input:focus::placeholder,
textarea:focus::placeholder {
  color: #000;
}

input:focus::-webkit-input-placeholder,
textarea:focus::-webkit-input-placeholder {
  color: #000;
}

input:focus:-ms-input-placeholder,
textarea:focus:-ms-input-placeholder {
  color: #000;
}

input:focus::-moz-placeholder,
textarea:focus::-moz-placeholder {
  color: #000;
}

@media screen and (max-width: 480px) {
  input,
select,
textarea {
    font-size: 16px !important;
  }
}
input[type=radio]:checked:after {
  border: 1px solid #0075ff;
}

::-webkit-scrollbar {
  width: 3px;
  height: 5px;
  border-radius: 99px;
}

::-webkit-scrollbar-thumb {
  background-color: #d6d6d6;
}

::-webkit-scrollbar-track-piece {
  background-color: transparent;
}

select {
  min-height: 35px;
  cursor: pointer;
  border: 1px solid #ccc;
}

.hide {
  display: none;
}

.user_editor {
  width: 100%;
  height: auto;
  overflow: hidden;
  position: static;
}

.user_editor img {
  max-width: 100%;
  height: auto !important;
}

.user_editor table {
  width: 100%;
}

.inner {
  max-width: 1400px;
  margin: auto;
  width: 100%;
}

@media screen and (max-width: 1400px) {
  .inner {
    padding: 0 20px;
  }
}
@media screen and (max-width: 480px) {
  .inner {
    padding: 0 15px;
  }
}
.max400 {
  max-width: 400px;
}

@media screen and (max-width: 480px) {
  .max400 {
    padding: 0 20px;
  }
}
.show-1200,
.show-1024,
.show-800,
.show-768 {
  display: none;
}

@media screen and (max-width: 1024px) {
  .show-1024 {
    display: block;
  }
}
@media screen and (max-width: 800px) {
  .show-800 {
    display: block;
  }
}
@media screen and (max-width: 768px) {
  .show-768 {
    display: inline-block;
  }
}
@media screen and (max-width: 1200px) {
  .show-1200 {
    display: inline-block;
  }
}
.hide-768,
.hide-1024,
.hide-1200 {
  display: block;
}

@media screen and (max-width: 1024px) {
  .hide-1024 {
    display: none;
  }
}
@media screen and (max-width: 768px) {
  .hide-768 {
    display: none;
  }
}
@media screen and (max-width: 1201px) {
  .hide-1200 {
    display: none;
  }
}
.font35 {
  font-size: 35px;
  color: #000;
  margin: 0px 0 50px;
  padding-bottom: 25px;
  border-bottom: 1px solid #e8e8e8;
  width: 100%;
}
@media screen and (max-width: 1200px) {
  .font35 {
    margin: 0px 0 30px;
    font-size: 28px;
  }
}
@media screen and (max-width: 600px) {
  .font35 {
    margin: 0px 0 30px;
    font-size: 28px;
    line-height: 35px;
  }
}
.font35 ~ .text {
  font-size: 18px;
  color: #666;
  line-height: 24px;
  margin-bottom: 20px;
}

.form-box .text {
  font-size: 14px;
  color: #666;
  line-height: 24px;
  margin-bottom: 20px;
  display: flex;
}

.line-flex {
  display: flex;
  margin-bottom: 5px;
  color: #000;
}

.tip-icon {
  color: #006dc8;
  font-size: 15px;
  margin-right: 5px;
  display: inline-block;
}

input {
  border: 1px solid #b7b7b7;
  line-height: 40px;
}

textarea {
  border: 1px solid #b7b7b7;
}

.btn {
  padding: 0px 5%;
  color: #fff;
  background-color: #006dc8;
  border: 0;
  line-height: 45px;
  max-width: 200px;
  width: 45%;
  text-transform: uppercase;
  font-size: 15px;
  font-weight: 700;
  letter-spacing: 1px;
}
.btn:hover {
  background-color: #269dff;
  transition: 0.5s;
  -moz-transition: 0.5s;
  -webkit-transition: 0.5s;
  -o-transition: 0.5s;
}

.one-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.two-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: normal;
}

.sp-font {
  font-family: "Oswald", sans-serif;
}

body {
  font-family: arial, sans-serif;
}

header {
  width: 100%;
  background-color: white;
  z-index: 8;
  padding: 35px 0 20px;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 5;
}
header > .inner {
  display: inline-flex;
  justify-content: space-between;
  max-width: 100%;
  padding: 0 2.5%;
}

.logo {
  max-width: 288px;
  display: inline-block;
  margin-right: 35px;
}

.logo a {
  display: block;
}

.logo img {
  width: 100%;
  max-height: 50px;
}

.nav-bar {
  cursor: pointer;
  position: relative;
  top: 12px;
}
@media screen and (max-width: 480px) {
  .nav-bar {
    margin-right: 5px;
  }
}
.nav-bar span, .nav-bar .form-box .code, .form-box .nav-bar .code {
  width: 20px;
  height: 3px;
  margin-bottom: 5px;
  background: #666;
  display: block;
}
.nav-bar.close span, .nav-bar.close .form-box .code, .form-box .nav-bar.close .code {
  opacity: 0;
  position: relative;
  transition: 0.3s;
  -moz-transition: 0.3s;
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  background: #006dc8;
}
.nav-bar.close span:first-of-type, .nav-bar.close .form-box .code:first-of-type, .form-box .nav-bar.close .code:first-of-type {
  opacity: 1;
  transform: rotate(45deg);
  top: 8px;
}
.nav-bar.close span:last-of-type, .nav-bar.close .form-box .code:last-of-type, .form-box .nav-bar.close .code:last-of-type {
  opacity: 1;
  transform: rotate(-45deg);
  top: -8px;
}

.top-menu {
  margin-top: 25px;
  width: calc(100% - 290px);
  text-transform: uppercase;
  justify-content: flex-end;
}
@media screen and (max-width: 1400px) {
  .top-menu {
    margin-top: 20px;
    font-size: 14px;
  }
}
@media screen and (min-width: 1200px) {
  .top-menu {
    display: flex !important;
  }
  .top-menu li {
    font-weight: 700;
    margin-left: 2.5%;
    display: inline-block;
    position: relative;
  }
  .top-menu li > a {
    line-height: 40px;
    display: block;
  }
  .top-menu li:hover > a {
    color: #006dc8;
    transition: 0.5s;
    -moz-transition: 0.5s;
    -webkit-transition: 0.5s;
    -o-transition: 0.5s;
  }
  .top-menu li:hover .menu-drop {
    height: auto;
    opacity: 1;
    border: 1px solid #eee;
  }
}
.top-menu .menu-drop {
  text-transform: initial;
}
@media screen and (min-width: 1201px) {
  .top-menu .menu-drop {
    height: 0;
    overflow: hidden;
    transition: 0.2s;
    -moz-transition: 0.2s;
    -webkit-transition: 0.2s;
    -o-transition: 0.2s;
    position: absolute;
    left: 0;
    opacity: 0;
    z-index: 1;
    border: 1px solid transparent;
  }
  .top-menu .menu-drop a {
    font-size: 12px;
    width: 180px;
    background: #fff;
    font-weight: 100;
    padding: 12px 15px;
    border-bottom: 1px solid #eee;
    color: #666;
    line-height: 18px;
    display: block;
  }
  .top-menu .menu-drop a:last-of-type {
    border-bottom: 0;
  }
  .top-menu .menu-drop a:hover {
    background: #eee;
    color: #006dc8;
  }
}
@media screen and (max-width: 1200px) {
  .top-menu {
    position: fixed;
    flex-wrap: wrap;
    background: #fff;
    width: 100%;
    left: 0;
    margin: 0;
    top: 65px;
    display: none;
    border: 1px solid #eee;
    overflow-y: scroll;
    max-height: calc(100% - 65px);
  }
  .top-menu .open {
    display: none;
  }
  .top-menu li {
    display: block;
    width: 100%;
    margin: 0;
    border-bottom: 1px solid #eee;
  }
  .top-menu li .drop {
    padding: 15px 20px;
    display: block;
    border-bottom: 1px solid #eee;
    line-height: inherit;
    position: relative;
  }
  .top-menu li .fas {
    position: absolute;
    right: 15px;
    top: calc(50% - 7px);
    color: #006dc8;
  }
  .top-menu .menu-drop {
    display: none;
  }
  .top-menu .menu-drop a {
    padding: 15px 35px;
    display: block;
    font-weight: 400;
    border-bottom: 1px solid #eee;
    background: #f9f9f9;
    color: #8a8a8a;
  }
  .top-menu .menu-drop a:last-of-type {
    border-bottom: 0;
  }
}

@media screen and (max-width: 1400px) {
  .logo {
    max-width: 200px;
  }
}
@media screen and (max-width: 1200px) {
  .logo {
    max-width: 175px;
  }

  header {
    padding: 17px 0 17px;
  }

  .m-bg {
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(109, 132, 150, 0.5);
    left: 0;
    top: 0;
    display: none;
    z-index: 3;
  }
}
.shortcut {
  background: #006dc8;
  font-size: 15px;
  position: -webkit-sticky;
  position: sticky;
  top: 120px;
  z-index: 3;
}
@media screen and (max-width: 1400px) {
  .shortcut {
    top: 115px;
  }
}
@media screen and (max-width: 1200px) {
  .shortcut {
    top: 65px;
  }
}

.shortcut > .inner {
  display: flex;
}

@media screen and (max-width: 1400px) {
  .shortcut > .inner {
    padding: 0;
  }
}
.shortcut img {
  width: 20px;
  margin-right: 5px;
  filter: grayscale(1) invert(1) contrast(6);
}

.shortcut li {
  width: 25%;
  display: inline-block;
  position: relative;
}

.shortcut li:hover,
.shortcut li.active {
  background: #eee;
  transition: 0.5s;
}

.shortcut li:hover a,
.shortcut li.active a {
  color: #006dc8;
  transition: 0.5s;
}

.shortcut li:hover img,
.shortcut li.active img {
  filter: inherit;
  transition: 0.5s;
}

.shortcut li:hover + li:after,
.shortcut li.active + li:after {
  opacity: 0;
}

.shortcut a {
  color: #fff;
  text-align: center;
  padding: 17px 0;
  position: relative;
  display: block;
}

.shortcut li:not(:first-of-type):after {
  content: "";
  width: 1px;
  height: 25px;
  background: rgba(255, 255, 255, 0.5);
  position: absolute;
  left: 0;
  top: 15px;
  display: block;
}

@media screen and (max-width: 650px) {
  .shortcut > .inner {
    flex-wrap: wrap;
  }

  .shortcut li {
    width: 50%;
  }

  .shortcut li:nth-of-type(odd) {
    border-right: 1px solid rgba(255, 255, 255, 0.5);
  }

  .shortcut li:nth-of-type(-n+2) {
    border-bottom: 1px solid rgba(255, 255, 255, 0.5);
  }

  .shortcut li:not(:first-of-type):after {
    display: none;
  }

  .shortcut a {
    padding: 13px 0;
    font-size: 13px;
  }

  .shortcut img {
    width: 15px;
  }
}
.bread {
  font-size: 12px;
  color: #999;
  border: 1px solid #e2e2e2;
  border-left: 0;
  border-right: 0;
  padding: 20px 0;
  background: #fff;
}

.bread a {
  color: #999;
}

.bread-space {
  margin: 0 10px;
}

footer .inner {
  padding: 75px 0 85px;
  display: flex;
  max-width: 1250px;
}

footer .inner > div {
  width: 33%;
}

.footer-btn {
  width: 100%;
  padding-right: calc(33% - 260px);
  font-size: 16px;
  letter-spacing: 1px;
}

.footer-btn a {
  display: block;
  background: #006dc8;
  color: #fff;
  margin-bottom: 38px;
}

.footer-btn a:hover {
  background: #eee;
  transition: 0.5s;
  color: #006dc8;
  transition: 0.5s;
}

.footer-btn a:hover img {
  filter: inherit;
  transition: 0.5s;
}

.footer-btn img {
  width: 20px;
  height: 50px;
  filter: grayscale(1) invert(1) contrast(6);
  margin: 0 15px;
}

.footer-btn span, .footer-btn .form-box .code, .form-box .footer-btn .code {
  line-height: 50px;
  border-left: 1px solid rgba(225, 225, 225, 0.4);
  display: inline-block;
  padding: 0 20px;
}

.footer-resources {
  width: 100%;
  padding-right: calc(33% - 370px);
}
.footer-resources a {
  font-size: 18px;
  padding: 22px 0 22px 30px;
  width: 100%;
  display: inline-block;
  color: #666;
  border-bottom: 1px solid #ccc;
  position: relative;
}
.footer-resources a:last-of-type {
  border-bottom: 0px;
}
.footer-resources a:before {
  content: "";
  width: 10px;
  height: 2px;
  background: #006dc8;
  display: block;
  position: absolute;
  left: 0;
  top: 29px;
}
.footer-resources a:hover {
  color: #006dc8;
  transition: 0.3s;
}

.footer-contact {
  padding-left: calc(33% - 335px);
}

.footer-contact img {
  width: 20px;
  margin-right: 20px;
}

.footer-contact .title {
  margin: 28px 0 15px;
  font-size: 18px;
}

.footer-contact .title + div {
  display: flex;
  color: #999;
  font-size: 15px;
}
.footer-contact .title + div a {
  color: #999;
  font-size: 15px;
  margin: 0;
}

.footer-contact a {
  display: flex;
  font-size: 24px;
  margin: 20px 0;
  color: #006dc8;
}
.footer-contact a:hover {
  opacity: 0.7;
  transition: 0.5s;
  -moz-transition: 0.5s;
  -webkit-transition: 0.5s;
  -o-transition: 0.5s;
}

.footer-title {
  font-size: 26px;
  color: #006dc8;
  margin-bottom: 8px;
}

.copyright {
  font-size: 13px;
  color: #b1b1b1;
  padding: 28px 0;
  background: #666;
  text-align: center;
}
@media screen and (max-width: 410px) {
  .copyright {
    font-size: 0;
  }
}

.copyright span, .copyright .form-box .code, .form-box .copyright .code {
  margin-right: 15px;
  font-size: 13px;
}

.copyright a {
  color: #b1b1b1;
  margin-left: 15px;
  font-size: 13px;
}

.copyright a:hover {
  color: #fff;
}

@media screen and (max-width: 1200px) {
  footer .inner {
    padding: 5% 20px;
    flex-wrap: wrap;
    max-width: 850px;
  }

  footer .inner > div {
    width: 50%;
  }

  .footer-btn {
    display: none;
  }

  .footer-resources {
    min-width: 370px;
    padding-right: calc(50% - 370px);
  }

  .footer-contact {
